{
	"name": "UMiBO",
	"description": "Turns your new tab into a virtual desktop to manage bookmarks.",
	"homepage_url": "https://github.com/Tsugumatsu/ff-umibo",
	"manifest_version": 2,
	"version": "0.85",
	"icons": {
		"48": "umibo.svg",
		"96": "umibo.svg"
	},
	"chrome_url_overrides" : {
		"newtab": "newTab.html"
	},
	"chrome_settings_overrides" : {
		"homepage": "newTab.html"
	},
	"permissions": [
		"bookmarks",
		"storage"
	],
	"options_ui": {
		"page": "options.html"
	},
	"applications": {
		"gecko": {
			"id": "clessili@gmail.com"
		}
	}
}
